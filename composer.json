{
    "name": "viachviach/availability-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": ["handler"],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "predis/predis": "^1.1",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.57",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AvailabilityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvailabilityBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": {
            "codestyle": "@codestyle",
            "static-analysis": "@static-analysis"
        },
        "fix": "phpcbf",
        "codestyle": "@php vendor/bin/phpcs"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability":  "dev",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
